{{#match home "one"}}
<div class="hero-area relative overflow-hidden py-12 md:py-16 lg:py-24 bg-gradient-to-b from-primary-50/50 to-white">
   <div class="container mx-auto">
      <div class="hero-box-1 grid items-center sm:grid-cols-2 lg:grid-cols-12 gap-10 lg:gap-24">
         <div class="hero-cotent relative lg:col-span-7 order-2 sm:order-1">
            <h1 class="font-serif text-5xl lg:text-6xl text-gray-900 font-bold mb-6 !leading-tight tracking-tight">
               {{@site.title}}
            </h1>
            {{#if @site.description}}
            <p class="text-xl text-gray-600 font-normal mb-8 leading-relaxed">{{@site.description}}</p>
            {{/if}}
            <div class="flex flex-wrap gap-4">
               <a href="/contact-us/"
                  class="hero-btn-1 inline-flex items-center justify-center text-base text-white font-semibold bg-primary-600 hover:bg-primary-700 shadow-lg hover:shadow-xl hover:-translate-y-0.5 transition-all duration-300 py-3.5 px-8 rounded-full">
                  {{t "contact-us"}}
               </a>
               <a href="/courses"
                  class="hero-btn-2 inline-flex items-center justify-center px-8 py-3.5 font-semibold text-base text-gray-700 glass-apple-card hover:text-primary-600 shadow-sm hover:shadow-md hover:-translate-y-0.5 transition-all duration-300 rounded-full">
                  {{t "view-courses"}}
               </a>
            </div>
         </div>
         {{#if @site.cover_image}}
         <div class="feature-image relative lg:col-span-5 order-1 sm:order-2">
            <div
               class="relative rounded-2xl shadow-2xl overflow-hidden transform hover:scale-[1.02] transition-transform duration-500">
               <img class="w-full h-auto object-cover" width="1200" height="630" src="{{img_url @site.cover_image size="
                  xl"}}" srcset="{{img_url @site.cover_image size=" m"}} 600w, {{img_url @site.cover_image size="l" }}
                  1000w, {{img_url @site.cover_image size="xl" }} 1200w" sizes="(max-width: 768px) 100vw, 50vw"
                  alt="{{@site.title}}" loading="eager">
            </div>
         </div>
         {{/if}}
      </div>
   </div>
</div>
{{/match}}
{{#match home "two"}}
<div class="hero-area relative overflow-hidden py-16 md:py-20 lg:py-32 bg-gradient-to-b from-primary-50/30 to-white">
   <div class="container mx-auto">
      <div class="hero-box-2">
         <div class="hero-cotent text-center relative max-w-4xl mx-auto">
            <h1 class="font-serif text-5xl lg:text-6xl text-gray-900 font-bold mb-8 !leading-tight tracking-tight">
               {{@site.title}}
            </h1>
            {{#if @site.description}}
            <h4 class="text-xl text-gray-600 font-normal mb-12 leading-relaxed">{{@site.description}}</h4>
            {{/if}}
            <a href="#/portal/signup" class="subscribe-form block max-w-2xl mx-auto">
               <div class="md:flex gap-4 relative p-2 bg-white rounded-full shadow-xl border border-gray-100">
                  <span type="search"
                     class="block px-6 py-4 w-full text-base text-gray-600 bg-transparent rounded-full focus:text-gray-900 placeholder-gray-400 outline-none">{{t
                     "Enter your email"}}</span>
                  <button type="submit"
                     class="text-base text-white font-bold bg-primary-600 hover:bg-primary-700 transition-all duration-300 rounded-full w-full md:w-auto px-10 py-4 shadow-md hover:shadow-lg whitespace-nowrap">{{t
                     "Subscribe"}}</button>
               </div>
            </a>
         </div>
      </div>
   </div>
</div>
{{/match}}
{{#match course_style "image_on_left"}}
<div class="hero-area relative py-8 md:py-10 lg:py-16 image-on-right">
   <div class="container mx-auto">
      <div class="hero-box-3 grid gap-8 grid-cols-1 lg:grid-cols-12 items-center text-left">
         <div class="lg:col-span-7 lg:pr-10 order-2">
            <div
               class=" hero-content flex flex-wrap content-center relative top-0 bottom-0 left-0 max-w-[860px] z-10 flex-col">
               {{#foreach tags visibility="all" limit="1"}}
               {{#get "posts" filter="tag:[hash-lesson]+tag:{{slug}}" order="title asc" include="tags,authors"
               limit="100" }}
               <p class="mb-2 text-base leading-relaxed text-blue-800 font-bold">{{plural posts.length empty='No Lessons
                  available' singular='Total % Lessons available' plural='Total % Lessons available'}}
               </p>
               {{/get}}
               {{/foreach}}
               <h1 class="font-serif !leading-tight text-4xl lg:text-5xl text-gray-800 font-bold  mb-6">{{title}}
               </h1>
               {{#if custom_excerpt}}
               <p class="text-xl text-gray-400 font-normal mb-6">{{custom_excerpt}}</p>
               {{/if}}


               <div class="w-full">
                  {{#primary_author}}
                  <div class="author flex items-center">
                     <img class="w-10 h-10 rounded-full object-cover" src="{{img_url profile_image size=" s"}}"
                        srcset="{{img_url profile_image size=" s"}} 1x, {{img_url profile_image size="m" }} 2x"
                        alt="{{name}}" loading="lazy">
                     <div class="course-content ml-2">
                        <h6 class="font-serif text-base text-gray-800 hover:text-blue-800 font-bold duration-300">
                           <a href="{{url}}">{{name}}</a>
                        </h6>
                        {{#if location}}
                        <p class="text-sm text-gray-400 font-normal opacity-60">{{location}}</p>
                        {{/if}}
                     </div>
                  </div>
                  {{/primary_author}}
               </div>
            </div>
         </div>
         {{#if feature_image}}
         <div class="lg:col-span-5 order-1">
            <div class="feature-image relative" data-aos="fade" data-aos-delay="100">
               <img class="w-full rounded-lg" srcset="{{img_url feature_image size=" m"}} 800w, {{img_url feature_image
                  size="l" }} 1200w, {{img_url feature_image size="xl" }} 1920w" sizes="(max-width: 1024px) 100vw, 50vw"
                  src="{{img_url feature_image size=" l"}}" alt="{{title}}" loading="eager">
            </div>
         </div>
         {{/if}}
      </div>
   </div>
</div>
{{/match}}

